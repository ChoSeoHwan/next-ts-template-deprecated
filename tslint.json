{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-react-hooks",
        "tslint-eslint-rules",
        "tslint-import-group-ordering",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "switch-default": false,
        "interface-name": true,
        "semicolon": false,
        "ordered-imports": false,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "no-console": false,
        "no-empty": false,
        "no-shadowed-variable": false,
        "object-literal-sort-keys": false,
        "import-group-ordering": {
            "severity": "warning",
            "options": {
                "imports-groups": [
                    {"name": "dependencies"},
                    {"name": "other"},
                    {"name": "library"},
                    {"name": "types"},
                    {"name": "constants"},
                    {"name": "modules"},
                    {"name": "sagas"},
                    {"name": "pages"},
                    {"name": "templates"},
                    {"name": "organisms"},
                    {"name": "molecules"},
                    {"name": "atoms"}
                ],
                "matching-rules": [
                    {
                        "type": "project",
                        "matches": "^(libs)",
                        "imports-group": "library"
                    },
                    {
                        "type": "project",
                        "matches": "^(constants)",
                        "imports-group": "constants"
                    },
                    {
                        "type": "project",
                        "matches": "^(types)",
                        "imports-group": "types"
                    },
                    {
                        "type": "project",
                        "matches": "^(modules)",
                        "imports-group": "modules"
                    },
                    {
                        "type": "project",
                        "matches": "^(sagas)",
                        "imports-group": "sagas"
                    },
                    {
                        "type": "project",
                        "matches": "^(pages)",
                        "imports-group": "pages"
                    },
                    {
                        "type": "project",
                        "matches": "^(components\/templates)",
                        "imports-group": "templates"
                    },
                    {
                        "type": "project",
                        "matches": "^(components\/organisms)",
                        "imports-group": "organisms"
                    },
                    {
                        "type": "project",
                        "matches": "^(components\/molecules)",
                        "imports-group": "molecules"
                    },
                    {
                        "type": "project",
                        "matches": "^(components\/atoms)",
                        "imports-group": "atoms"
                    },
                    {
                        "type": "dependencies",
                        "imports-group": "dependencies",
                        "disable-native-nodejs-modules": true,
                        "from-package.json": true
                    },
                    {
                        "type": "project",
                        "matches": ".*",
                        "imports-group": "other"
                    }
                ]
            }
        }
    },
    "rulesDirectory": []
}